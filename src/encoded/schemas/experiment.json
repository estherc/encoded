{
    "title": "ENCODE Biosample",
    "id": "/profiles/biosample.json",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "Schema for submitting an ENCODE Biosample",
    "type": "object",
    "required": ["lab_uuid", "award_uuid", "description"],
    "properties": {
        "assay_type": {
            "default": "",
            "description": "Assay Type of the experiment",
            "comment": "Assay Type",
            "title": "Assay Type",
            "type": "string",
            "enum" : [],
            "required": true
        },
        "lab_uuid": {
            "default": "ENCODE Project",
            "description": "Lab submitting the experiment",
            "comment": "Automatic submitters can supply a Lab UUID or Name",
            "title": "Lab",
            "type": "string",
            "format": "lab_link",
            "enum" : [],
            "required": true
        },
        "award_uuid": {
            "default": "ENCODE",
            "description": "Award (Grant) this submission is registered under",
            "comment": "Automatic submitters can supply an Award UUID, name, or number",
            "title": "Award Name",
            "type": "string",
            "format": "award_link",
            "enum" : [],
            "required": true
        },
        "description": {
            "default": "",
            "description": "A plain text description of the biosample",
            "title": "Description",
            "type": "string",
            "required": true
        }
    }
}