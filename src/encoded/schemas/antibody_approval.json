{
    "title": "ENCODE Antibody",
    "id": "/profiles/antibody_approval.json",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "Schema for submitting an ENCODE Antibody",
    "type": "object",
    "required": [],
    "properties": {
        "antibody_product_id": {
            "default": "",
            "description": "product id of the antibody",
            "title": "Product ID",
            "type": "string",
            "enum": [],
            "required": true
        },
        "antibody_lot_id": {
            "default": "",
            "description": "lot id of the antibody",
            "title": "Lot ID",
            "type": "string",
            "enum": [],
            "required": true
        },
        "source_uuid": {
            "description": "Source or Vendor of Biomaterials",
            "comment": "Automatic submitters can supply an ENCODE source UUID or unique Source name",
            "title": "Source",
            "type": "string",
            "format": "source_link",
            "enum": [] 
        }, 
        "clonality": {
            "default": "",
            "description": "clonality of the antibody",
            "title": "Clonality",
            "type": "string",
            "required": true
        },
        "host_organism": {
            "default": "",
            "description": "species",
            "title": "Species",
            "type": "string",
            "enum":[
                "Human",
                "Rabbit",
                "Rat",
                "Mouse",
                "Sheep",
                "Goat"
            ],
            "required": true
        },
        "iso_type": {
            "default": "",
            "description": "Iso Type of the antibody",
            "title": "ISO Type",
            "type": "string",
            "required": true
        },
        "purification": {
            "default": "",
            "description": "Purification of the antibody",
            "title": "Purification",
            "type": "string",
            "required": true
        },
        "target_uuid": {
            "default": "",
            "description": "target of the antibody",
            "title": "Target",
            "type": "string",
            "enum":[],
            "required": true
        }
    }
}